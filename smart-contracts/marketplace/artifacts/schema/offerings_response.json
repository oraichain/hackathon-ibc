{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OfferingsResponse",
  "type": "object",
  "required": [
    "offerings"
  ],
  "properties": {
    "offerings": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/QueryOfferingsResult"
      }
    }
  },
  "definitions": {
    "HumanAddr": {
      "type": "string"
    },
    "QueryOfferingsResult": {
      "type": "object",
      "required": [
        "contract_addr",
        "id",
        "price",
        "seller",
        "token_id"
      ],
      "properties": {
        "contract_addr": {
          "$ref": "#/definitions/HumanAddr"
        },
        "id": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "price": {
          "$ref": "#/definitions/Uint128"
        },
        "seller": {
          "$ref": "#/definitions/HumanAddr"
        },
        "token_id": {
          "type": "string"
        }
      }
    },
    "Uint128": {
      "type": "string"
    }
  }
}
